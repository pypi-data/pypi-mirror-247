<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="pas.plugins.skipauthentication"
  purge="False">

  <configlet
    title="Skip Authentication"
    action_id="skipauthentication"
    appId="pas.plugins.skipauthentication"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@skipauthentication-controlpanel"
    icon_expr="string:$portal_url/group.png"
    visible="True"
    i18n:attributes="title">
      <permission>Manage portal</permission>
  </configlet>

</object>
